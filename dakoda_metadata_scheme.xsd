	<xs:complexType name="CorpusAdministrative">
		<xs:sequence>

			
			<xs:element name="corpus_admin_acronym" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Corpus acronym. An equivalent field in LC-meta is `corpus_acronym`. </xs:documentation>
					<xs:appinfo>
						<displayname>Akronym</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_name" type="xs:string" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The fully disabbreviated title of the corpus. Can occur multiple
						times for corpora with official names in multiple languages. An equivalent 
						field in LC-meta is `corpus_name`. </xs:documentation>
					<xs:appinfo>
						<displayname>Korpustitel ausgeschrieben</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_author" type="xs:string" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Corpus creator. An equivalent field in LC-meta is  `corpus_author`. 
					</xs:documentation>
					<xs:appinfo>
						<displayname>Verantwortliche/-r</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_availability" type="CORPUS_AVAILABILITY_TYPE" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Licensing terms and conditions for corpus. An equivalent field
						in LC-meta is `corpus_availability` </xs:documentation>
					<xs:appinfo>
						<displayname>Zugang und Lizenz</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_citationDocument" type="xs:string" minOccurs="1" maxOccurs="1" default="notAvailable">
				<xs:annotation>
					<xs:documentation>Information on how excerpts of the corpus should be
						referenced. This field has no counterpart in LC-meta . </xs:documentation>
					<xs:appinfo>
						<displayname>Referenz auf Textbelege</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_citeAs" type="NON_ZERO_LENGTH_STRING" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Corpus citation. An equivalent field in LC-meta is `corpus_cite_as`. </xs:documentation>
					<xs:appinfo>
						<displayname>Zitation des Korpus</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>



			<xs:element name="corpus_admin_contactMail" type="EMAIL_ADDRESS" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contact e-mail address . An equivalent field in LC-meta is 
						`corpus_contact_mail`.</xs:documentation>
					<xs:appinfo>
						<displayname>Emailadresse der/des Korpusverantwortlichen
							(Original-Version)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_contributor_dkd" type="DKD_CONTRIBUTOR" minOccurs="11"
				maxOccurs="11">

				<xs:annotation>
					<xs:documentation>Contributor (DAKODA version).
						LC-meta has the comparable field `corpus_contributor`. </xs:documentation>
					<xs:appinfo>
						<displayname>Mitwirkende (DAKODA-Version)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_contributor_orig" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contributor (original version). LC-meta has the comparable field `corpus_contributor`. </xs:documentation>
					<xs:appinfo>
						<displayname>Mitwirkende (Original-Version)</displayname>
					</xs:appinfo>
				</xs:annotation>

			</xs:element>

			<xs:element name="corpus_admin_dateOfPublication" type="DATE_OF_PUBLICATION">
				<xs:annotation>
					<xs:documentation>Publication year of the source version of the corpus. 
						An comparable field name in LC-meta is `corpus_date_of_publication`. </xs:documentation>
					<xs:appinfo>
						<displayname>Veröffentlichungsdatum</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_documentation" type="xs:string" minOccurs="1" maxOccurs="unbounded" default="notAvailable">

				<xs:annotation>
					<xs:documentation>Link to more extensive corpus documentation. An equivalent field in LC-meta is `corpus_documentation`. </xs:documentation>
					<xs:appinfo>
						<displayname>Korpushandbuch</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_fileFormat" type="xs:string" minOccurs="1"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>File format (DAKODA-Version). An equivalent field in LC-meta is `corpus_file_format`. </xs:documentation>
					<xs:appinfo>
						<displayname>Dateiformate</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>




			<xs:element name="corpus_admin_licence" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Licence name (DAKODA version).  An equivalent field in LC-meta is `corpus_licence`. </xs:documentation>
					<xs:appinfo>
						<displayname>Lizenz in DAKODA</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_licenceFulltext" type="xs:string" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Licence text (DAKODA version). There is no equivalent field in LC-meta.</xs:documentation>
					<xs:appinfo>
						<displayname>Lizenztext</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_licenceUrl" type="xs:anyURI" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Link to licence text. An equivalent field in LC-meta is `corpus_licence_url`. 
					</xs:documentation>
					<xs:appinfo>
						<displayname>Link zu Lizenztext</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_otherVersions" type="xs:string" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Associated version of the corpus. This will often be an earlier
						release of the corpus. An equivalent field in LC-meta is
						`corpus_other_versions`. </xs:documentation>
					<xs:appinfo>
						<displayname>andere Korpusversionen</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_pid_dkd" type="xs:anyURI" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Persistent identifier (DAKODA version). A similar field in LC-meta is  
						`corpus_pid`. <xs:appinfo><displayname>PID (DAKODA-Version)</displayname></xs:appinfo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_pid_orig" type="xs:anyURI" minOccurs="1" maxOccurs="1">
				<xs:annotation> 
					<xs:documentation>Persistent Identifier (original version). An equivalent field in LC-meta is  `corpus_pid`. </xs:documentation>
					<xs:appinfo>
						<displayname>PID (Originalversion)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_refArticle" type="xs:string" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Reference article. Such articles typically
						contain an extensive verbal description of the corpus and its design. 
						An equivalent field in LC-meta is `corpus_ref_article`. </xs:documentation>
					<xs:appinfo>
						<displayname>Referenzartikel</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_referencesOther" type="xs:string" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Further important publications or links to the corpus . 
						LC-meta has related metadata field `corpus_ref_article`. 
					</xs:documentation>
					<xs:appinfo>
						<displayname>weitere Referenz</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="corpus_admin_researchPaper" type="xs:string" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Further publications (original version). An equivalent field in LC-meta is  corpus_ref_article`. </xs:documentation>
					<xs:appinfo>
						<displayname>weitere Publikationen (Original-Version)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_admin_URL_download" type="xs:anyURI" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Link to corpus download (original version). There is no equivalent field in LC-meta.</xs:documentation>
					<xs:appinfo>
						<displayname>Korpusdownload (Original-Version)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_URLquery" type="xs:anyURI" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Link to corpus query (original version).</xs:documentation>
					<xs:appinfo>
						<displayname>Link zur Korpussuche (Original-Version)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="corpus_admin_version_orig" type="xs:string">
				<xs:annotation>
					<xs:documentation>Data version on which the DAKODA version is based: Version number, PID or download link with date. 
						An equivalent field in LC-meta is `corpus_version`. </xs:documentation>
					<xs:appinfo>
						<displayname>Korpusversion</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


		</xs:sequence>
	</xs:complexType>



