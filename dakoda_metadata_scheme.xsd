					<xs:appinfo>
						<displayname>Erstsprache(n)</displayname>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>


			<xs:element name="corpus_design_l1Type" type="L1_CONSTELLATION" minOccurs="1" maxOccurs="unbounded">

				<xs:annotation>
