<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
	xmlns:xsi="http://xml.org"
    xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" vc:minVersion="1.1">

	<xs:import namespace="http://xml.org"/>
	<xs:include schemaLocation="languages.xsd"/>
	<xs:include schemaLocation="countries.xsd"/>
	




	<xs:annotation>
		<xs:documentation>			<sectionHeader> ********* simpleTypes *************** </sectionHeader>
		</xs:documentation>
	</xs:annotation>



<!--mother; father; not specified attachment figure; none-->

	<xs:simpleType name="PARENTAL_RELATIVE">
		<xs:annotation>
			<xs:documentation>Relative who has a language of the learner as their L1.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="father"/>
			<xs:enumeration value="mother"/>
			<xs:enumeration value="not specified attachment figure"/>
			<xs:enumeration value="none"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="WORD_ORDER_TYPE">
		<xs:annotation>
			<xs:documentation>Dominant word order type according to WALS.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SOV"/>
			<xs:enumeration value="SVO"/>
			<xs:enumeration value="VSO"/>
			<xs:enumeration value="SVO ; VSO"/>
			<xs:enumeration value="SOV; SVO"/>
			<xs:enumeration value="no dominant order"/>
			<xs:enumeration value="unclear"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="TASK_STIMULUS_TYPE">
		<xs:annotation>
			<xs:documentation>Type of stimulus for the task.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="advertisement"/>
			<xs:enumeration value="article"/>
			<xs:enumeration value="articles"/>
			<xs:enumeration value="book"/>
			<xs:enumeration value="comic"/>
			<xs:enumeration value="description of a situation"/>
			<xs:enumeration value="diagram"/>
			<xs:enumeration value="essay"/>
			<xs:enumeration value="extract from a doctoral dissertation"/>
			<xs:enumeration value="extracts from articles"/>
			<xs:enumeration value="figure"/>
			<xs:enumeration value="text edited for teaching"/>
			<xs:enumeration value="form"/>
			<xs:enumeration value="interview"/>
			<xs:enumeration value="job advertisement"/>
			<xs:enumeration value="letter"/>
			<xs:enumeration value="list of words or expressions"/>
			<xs:enumeration value="map"/>
			<xs:enumeration value="oral instructions"/>
			<xs:enumeration value="picture(s)"/>
			<xs:enumeration value="questionnaire"/>
			<xs:enumeration value="quote"/>
			<xs:enumeration value="scene acted out"/>
			<xs:enumeration value="talks"/>
			<xs:enumeration value="video"/>
			<xs:enumeration value="written instruction"/>
			<xs:enumeration value="notAvailable"/>
			<xs:enumeration value="notApplicable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="FORMALITY_TYPE">
		<xs:annotation>
			<xs:documentation>Formality level of the task.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="informal"/>
			<xs:enumeration value="unmarked to informal"/>
			<xs:enumeration value="unmarked"/>
			<xs:enumeration value="unmarked to formal"/>
			<xs:enumeration value="formal"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="EXPOSURE_INSTITUTION">
		<xs:annotation>
			<xs:documentation>Type of institution where learner mainly learns/learnt the language.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="language school"/>
			<xs:enumeration value="school"/>
			<xs:enumeration value="university"/>
			<xs:enumeration value="other"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>
	



	<xs:simpleType name="TRG_LANG_INPUT_TYPE">
		<xs:annotation>
			<xs:documentation>Dominant word order type according to WALS.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="mainly without controlled teaching processes"/>
			<xs:enumeration value="mainly in controlled teaching contexts"/>
			<xs:enumeration value="hybrid"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="POSSIBILITIES_FOR_COMPARISONS">
		<xs:annotation>
			<xs:documentation>Possibilities for comparing tasks and time points</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="one task, done once"/>		
			<xs:enumeration value="one task, done repeatedly"/>
			<xs:enumeration value="several tasks, each done repeatedly" />
			<xs:enumeration value="several tasks, each done once"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="PERCENT">
		<xs:annotation>
			<xs:documentation>The percent type specifies a declimal value from 0.00 to 100.00
				.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="PERCENT_OR_NA">
		<xs:annotation>
			<xs:documentation>A decimal percent value or a specification of NA .
			</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="PERCENT NA_STRING"/>
	</xs:simpleType>



	<xs:simpleType name="PROFICIENCY_ASSESSMENT_METHOD">
		<xs:annotation>
			<xs:documentation>A type of proficiency assessment.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="independent instrument"/>
			<xs:enumeration value="total test score"/>
			<xs:enumeration value="other"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="PROFICIENCY_ASSIGNMENT_METHOD_TYPE">
		<xs:annotation>
			<xs:documentation>Method used for proficiency assessment.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="score on text"/>
			<xs:enumeration value="teacher's evaluation"/>
			<xs:enumeration value="post-hoc assignment"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>




	<xs:simpleType name="DATA_PRODUCTION_SETTING_CONCEPTUAL_MODE">
		<xs:annotation>
			<xs:documentation>A list of possible ceonceptual modes in which the corpus data was
				produced.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="spoken"/>
			<xs:enumeration value="written"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>




	<xs:simpleType name="BOOL_OR_NA">
		<xs:annotation>
			<xs:documentation>A boolean value or a specification of NA . </xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:boolean NA_STRING"/>
	</xs:simpleType>


	<xs:simpleType name="DATE_OR_NA">
		<xs:annotation>
			<xs:documentation>A date value or a specification of NA . </xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:date NA_STRING"/>
	</xs:simpleType>

	<xs:simpleType name="ACQ_REGION_DESCRIPTOR">
		<xs:restriction base="xs:string">
			<xs:enumeration value="within German-speaking countries"/>
			<xs:enumeration value="outside German-speaking countries"/>
			<xs:enumeration value="hybrid"/>
		</xs:restriction>
	</xs:simpleType>
	

	<xs:simpleType name="COUNTRY_TYPE_OR_NA">
		<xs:annotation>
			<xs:documentation>A country specified as COUNTRY_TYPE or a string indicating that no
				value is available. </xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="COUNTRY_TYPE ACQ_REGION_DESCRIPTOR NA_STRING"/>
	</xs:simpleType>



	<xs:simpleType name="STRING_OR_NUMBER">
		<xs:annotation>
			<xs:documentation>Either a positive int, a decimal or a string. </xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:positiveInteger xs:decimal xs:string"/>
	</xs:simpleType>





	<xs:simpleType name="INTERACTION_TYPES">
		<xs:annotation>
			<xs:documentation>A specification of the language combinations used .</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="only L1 speakers">
				<xs:annotation>
					<xs:documentation>Only L1 speakers take part in the interaction.
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="only L2 speakers">
				<xs:annotation>
					<xs:documentation>Only L2 speakers are part of the interaction.
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L1 and L2 speakers mixed">
				<xs:annotation>
					<xs:documentation>A mix of L1- and L2-speakers is part of the
						interaction. </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="notAvailable"/>
			<xs:enumeration value="notApplicable"/>
		</xs:restriction>
	</xs:simpleType>









	<xs:simpleType name="STORAGE_UNIT">
		<xs:restriction base="xs:string">
			<xs:enumeration value="KB"/>
			<xs:enumeration value="MB"/>
			<xs:enumeration value="GB"/>
			<xs:enumeration value="TB"/>
			<xs:enumeration value="PB"/>
		</xs:restriction>
	</xs:simpleType>





	<xs:simpleType name="DATA_PRODUCTION_SETTING_MODE">
		<xs:annotation>
			<xs:documentation>A list of possible modes in which the corpus data was
				produced.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="spoken"/>
			<xs:enumeration value="written"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="PROFICIENCY_LEVEL">
		<xs:annotation>
			<xs:documentation>A list of possible coarse proficiency levels specified as CEFR levels
				or ranges. Including value "notAvailable"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A1"/>
			<xs:enumeration value="A2"/>
			<xs:enumeration value="B1"/>
			<xs:enumeration value="B2"/>
			<xs:enumeration value="C1"/>
			<xs:enumeration value="C2"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="CORPUS_AVAILABILITY_TYPE">
		<xs:annotation>
			<xs:documentation>A list of license types by which a corpus may be available, if at
				all</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="closed">
				<xs:annotation>
					<xs:documentation>access only for members of the DAKODA Project</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="restricted">
				<xs:annotation>
					<xs:documentation>access restricted to members of German academic institutions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>

			<xs:enumeration value="special restrictions">
				<xs:annotation>
					<xs:documentation>special restrictions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="open">
				<xs:annotation>
					<xs:documentation>open; all CC-licenses</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="notAvailable">
				<xs:annotation>
					<xs:documentation>information is not available</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="STUDY_DESIGN">
		<xs:annotation>
			<xs:documentation>The study design under which the corpus data was
				produced.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="longitudinal"/>
			<xs:enumeration value="pseudo-longitudinal"/>
			<xs:enumeration value="cross-sectional"/>
			<!--xs:enumeration value="longitudinal; cross-sectional"/--><!-- Di 07 Okt 2025 ( KW40 ) -->
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="EDUCATIONAL_STAGE">
		<xs:annotation>
			<xs:documentation>The education stage the learner is in at the time of data
				collection.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="early childhood"/>
			<xs:enumeration value="primary"/>
			<xs:enumeration value="lower secondary"/>
			<xs:enumeration value="upper secondary"/>
			<xs:enumeration value="post-secondary non-tertiary"/>
			<xs:enumeration value="short-cycle tertiary"/>
			<xs:enumeration value="Bachelor"/>
			<xs:enumeration value="Master"/>
			<xs:enumeration value="Doctorate"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="TOPIC_TYPE">
		<xs:annotation>
			<xs:documentation>A list of topic types that may be assigned to
				texts.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="domestic"/>
			<xs:enumeration value="daily activities"/>
			<xs:enumeration value="business / work place"/>
			<xs:enumeration value="science"/>
			<xs:enumeration value="education / academic"/>
			<xs:enumeration value="government / legal / politics"/>
			<xs:enumeration value="religion"/>
			<xs:enumeration value="sports"/>
			<xs:enumeration value="art / entertainement"/>
			<xs:enumeration value="other"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="LEARNER_TASK_TYPE">
		<xs:annotation>
			<xs:documentation>Type of task used in collecting the data</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="book review"/>
			<xs:enumeration value="consultation"/>
			<xs:enumeration value="conversation"/>
			<xs:enumeration value="description"/>
			<xs:enumeration value="essay"/>
			<xs:enumeration value="instruction"/>
			<xs:enumeration value="interview"/>
			<xs:enumeration value="letter"/>
			<xs:enumeration value="map task"/>
			<xs:enumeration value="narrow elicitation task"/>
			<xs:enumeration value="post in a forum"/>
			<xs:enumeration value="problem sovling"/>
			<xs:enumeration value="report"/>
			<xs:enumeration value="story"/>
			<xs:enumeration value="summary"/>
			<xs:enumeration value="translation"/>
			<xs:enumeration value="notAvailable"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DKD_CONTRIBUTOR">
		<xs:annotation>
			<xs:documentation>A person working for the Dakoda project. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Jamila Bläsing"/>
			<xs:enumeration value="Luise Böttcher"/>
			<xs:enumeration value="Shanny Druker"/>
			<xs:enumeration value="Lisa Lenort"/>
			<xs:enumeration value="Annette Portmann"/>
			<xs:enumeration value="Christine Renker"/>
			<xs:enumeration value="Josef Ruppenhofer"/>
			<xs:enumeration value="Matthias Schwendemann"/>
			<xs:enumeration value="Iulia Sucutardean"/>
			<xs:enumeration value="Katrin Wisniewski"/>
			<xs:enumeration value="Torsten Zesch"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="DKD_PROJECT_TYPE">
		<xs:annotation>
			<xs:documentation>The type of funding that supported the DADKOA project.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Bundesministerium für Forschung, Technologie und Raumfahrt (BMFTR)"/>

		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="DKD_PROJECT_NAME">
		<xs:annotation>
			<xs:documentation>Vollständiger Name des DAKODA-Projekts </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration
				value="Datenkompetenzen in DaF/DaZ: Exploration sprachtechnologischer Ansätze zur Analyse von L2-Erwerbsstufen in Lernerkorpora des Deutschen"/>

		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="DKD_PROJECT_HEAD">
		<xs:annotation>
			<xs:documentation>A principal investigator of the Dakoda project. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Katrin Wisniewski"/>
			<xs:enumeration value="Torsten Zesch"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="YEAR_MONTH">
		<xs:annotation>
			<xs:documentation>A specification of a particular month in a specified year such as
				1989-05</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}-\d{2}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="YEAR_MONTH_DAY">
		<xs:annotation>
			<xs:documentation>A specification of a particular month and day in a specified year such as
				1989-05-01</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}-\d{2}-\d{2}"/>
		</xs:restriction>
	</xs:simpleType>
	

	<xs:simpleType name="YEAR_ONGOING">
		<xs:annotation>
			<xs:documentation>A specification of a period beginning from a specific year that is said to be ongoing.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}-ongoing"/>
		</xs:restriction>
	</xs:simpleType>
	


	<xs:simpleType name="DATE_OF_PUBLICATION">
		<xs:annotation>
			<xs:documentation>Date of publication given as YYYY, YYYY-MM or YYYY-MM-DD. . </xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:gYear YEAR_MONTH_DAY YEAR_SPAN YEAR_MONTH NA_STRING"/>
	</xs:simpleType>

	<xs:simpleType name="NA_STRING">
		<xs:annotation>
			<xs:documentation>A string indicating that no value is available for a metadatum or
				that the metadatum is not applicable. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="notAvailable"/>
			<xs:enumeration value="notApplicable"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="POS_FLOAT">
		<xs:restriction base="xs:float">
			<xs:minExclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="NON_NEG_NUMBER_OR_NA">
		<xs:annotation>
			<xs:documentation>A non-negative integer or float or a string indicating that no value is avaiablle
			</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:nonNegativeInteger LimitedDouble NA_STRING"/>
	</xs:simpleType>

	<xs:simpleType name="LimitedDouble">
		<xs:restriction base="xs:double">
			<xs:minExclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	

	<xs:simpleType name="POS_INT_OR_NA">
		<xs:annotation>
			<xs:documentation>A positive integer or a string indicating that no value is available
			</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:positiveInteger NA_STRING"/>
	</xs:simpleType>
	
	

	<xs:simpleType name="POS_NUMBER_OR_NA">
		<xs:annotation>
			<xs:documentation>A positive integer or float or a string indicating that no value is avaiablle
			</xs:documentation>
		</xs:annotation>
		<!--xs:union memberTypes="xs:positiveInteger POS_FLOAT NA_STRING"/-->
		<!--Di 07 Okt 2025 ( KW40 ) -->
		<xs:union memberTypes="xs:nonNegativeInteger POS_FLOAT NA_STRING"/>
	</xs:simpleType>
	

	<xs:simpleType name="nonNegativeInteger" id="nonNegativeInteger">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="YEAR_SPAN">
		<xs:annotation>
			<xs:documentation>A span of years such as 1989-1992 </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}-\d{4}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="YEAR_OR_YEAR_SPAN">
		<xs:union memberTypes="xs:gYear YEAR_SPAN NA_STRING YEAR_ONGOING"/>
	</xs:simpleType>

	<xs:simpleType name="INT_SPAN_HUNDREDS">
		<xs:annotation>
			<xs:documentation>A span of integers in the range 0-999. Used for the union type
				TASK_DURATION.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,3}-\d{1,3}"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="TASK_DURATION">
		<xs:union memberTypes="xs:positiveInteger INT_SPAN_HUNDREDS NA_STRING"/>
	</xs:simpleType>


	<xs:simpleType name="INT_SPAN">
		<xs:annotation>
			<xs:documentation>A span of integers such as 4-6. Used for the union type
				NUMBER_PARTICIPANTS </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,2}-\d{1,2}"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="POS_INT_OR_INT_SPAN_OR_NA">
		<xs:annotation>
			<xs:documentation>A positive integer or an integer span or a string indicating that no value is avaiablle
			</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:positiveInteger INT_SPAN NA_STRING"/>
	</xs:simpleType>
	

	<xs:simpleType name="NUMBER_N_TYPE">
		<xs:restriction base="xs:string">
			<xs:enumeration value="n"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="NUMBER_PARTICIPANTS">
		<xs:union memberTypes="xs:integer INT_SPAN NUMBER_N_TYPE NA_STRING"/>
	</xs:simpleType>





	<xs:simpleType name="CORPUS_GROUP">
		<xs:restriction base="xs:string">
			<xs:enumeration value="CDLK"/>
			<xs:enumeration value="KiezDeutsch-Korpus"/>
			<xs:enumeration value="DISKO"/>
			<xs:enumeration value="EURAC-Korpora"/>
			<xs:enumeration value="Falko"/>
			<xs:enumeration value="FD-Lex"/>
			<xs:enumeration value="HaMaTaC"/>
			<xs:enumeration value="HaMoTiC"/>
			<xs:enumeration value="notApplicable"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="AMERICAN_GRADE">
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="D"/>
			<xs:enumeration value="F"/>
		</xs:restriction>
	</xs:simpleType>



	<xs:simpleType name="GERMAN_GRADE">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
		</xs:restriction>
