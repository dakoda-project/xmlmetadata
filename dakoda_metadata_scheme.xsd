<!-- Annotation-->


	<xs:complexType name="Annotation">
		<xs:sequence>

			<xs:element name="annotation_automatic" type="xs:boolean" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Was the annotation process fully automatic? An equivalent field in LC-meta is `annotation_automatic`. </xs:documentation>
